# --- ENV ---
export LOCAL=/usr/local
export LIB=/usr/lib
export KUBECONFIG=$WHOME/.kube/config
export BUILDS=$HOME/builds
export JAVA_HOME=$LOCAL/jdk-11.0.4+11
export GOOGLE_CLOUD_SDK=/usr/local/google-cloud-sdk
export SSH_AUTH_SOCK=$HOME/.gnupg/S.gpg-agent.ssh
export VISUAL=vim
export EDITOR=$VISUAL
export ASDF=$HOME/.asdf
export GOVERSION=1.13.5

export DOT=$HOME/.dotfiles
export FUNCTIONS=$DOT/.functions
export PATHS=$DOT/.paths
export ALIASES=$DOT/.aliases
export COMPLETION=$DOT/.completion
export SETUP=$DOT/.setup

# --- PATH ---
export PATH=$PATH:$HOME/.cargo/bin
export PATH=$PATH:$HOME/.npm/.global/bin
export PATH=$PATH:$BUILDS/skaffold/out
export PATH=$PATH:$JAVA_HOME/bin
export PATH=$PATH:$LIB/protoc/bin
export PATH=$GOOGLE_CLOUD_SDK/bin:$PATH

# --- AUTO COMPLETE ---
source <(kubectl completion zsh)
